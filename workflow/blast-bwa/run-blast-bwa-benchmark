echo "bwa 1017052009 (8 of 95)"
bwa mem -t 1 -R "@RG\tID:1017052009\tSM:1017052009" "$datadir/Oar_v3.1.fa" "$demultidir/1017052009_TGTCACA_psti.R1.fastq.gz" 2>$bwadir/1017052009.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1017052009.bam >"$bwadir/1017052009.out" 2>"$bwadir/1017052009.out"
