#!/bin/sh
# needs to run in conda environment blast-bwa

test -n "$INPUT_DATA_ROOT_DIR" || {
    echo >&2 "fatal error: missing environment variable INPUT_DATA_ROOT_DIR - source the top-level environment file"
    exit 1
}
test -n "$OUTPUT_DATA_ROOT_DIR" || {
    echo >&2 "fatal error: missing environment variable OUTPUT_DATA_ROOT_DIR - source the top-level environment file"
    exit 1
}

datadir=$INPUT_DATA_ROOT_DIR/blast-bwa
outdir=$OUTPUT_DATA_ROOT_DIR/blast-bwa

test -d "$datadir" || {
    echo >&2 "fatal error: missing data directory $datadir - unpack the data tarball"
    exit 1
}

mkdir -p "$outdir" || {
    echo >&2 "fatal error: failed to create output directory $outdir"
    exit 1
}

rm -rf "$outdir"/*

# check all expected programs available
for prog in fastq_to_fasta bwa blastn samtools; do
    type $prog >/dev/null 2>&1 || {
        echo >&2 "fatal error: $prog not found - did you create and activate the blast-bwa conda environment?"
        exit 1
    }
done

demultidir="$datadir/sample_fastq.txt.gz_demulti"

# align using bwa
bwadir="$outdir/bwa"
mkdir -p "$bwadir"

echo "bwa 1016454388 (1 of 95)"
bwa mem -t 1 -R "@RG\tID:1016454388\tSM:1016454388" "$datadir/Oar_v3.1.fa" "$demultidir/1016454388_TTCTA_psti.R1.fastq.gz" 2>$bwadir/1016454388.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1016454388.bam >"$bwadir/1016454388.out" 2>"$bwadir/1016454388.out"
echo "bwa 1016463343 (2 of 95)"
bwa mem -t 1 -R "@RG\tID:1016463343\tSM:1016463343" "$datadir/Oar_v3.1.fa" "$demultidir/1016463343_TTACGA_psti.R1.fastq.gz" 2>$bwadir/1016463343.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1016463343.bam >"$bwadir/1016463343.out" 2>"$bwadir/1016463343.out"
echo "bwa 1016465443 (3 of 95)"
bwa mem -t 1 -R "@RG\tID:1016465443\tSM:1016465443" "$datadir/Oar_v3.1.fa" "$demultidir/1016465443_GAACGACCT_psti.R1.fastq.gz" 2>$bwadir/1016465443.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1016465443.bam >"$bwadir/1016465443.out" 2>"$bwadir/1016465443.out"
echo "bwa 1016465480 (4 of 95)"
bwa mem -t 1 -R "@RG\tID:1016465480\tSM:1016465480" "$datadir/Oar_v3.1.fa" "$demultidir/1016465480_CGAGGTGGA_psti.R1.fastq.gz" 2>$bwadir/1016465480.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1016465480.bam >"$bwadir/1016465480.out" 2>"$bwadir/1016465480.out"
echo "bwa 1016465560 (5 of 95)"
bwa mem -t 1 -R "@RG\tID:1016465560\tSM:1016465560" "$datadir/Oar_v3.1.fa" "$demultidir/1016465560_CACATGA_psti.R1.fastq.gz" 2>$bwadir/1016465560.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1016465560.bam >"$bwadir/1016465560.out" 2>"$bwadir/1016465560.out"
echo "bwa 1017052001 (6 of 95)"
bwa mem -t 1 -R "@RG\tID:1017052001\tSM:1017052001" "$datadir/Oar_v3.1.fa" "$demultidir/1017052001_ACCATGACA_psti.R1.fastq.gz" 2>$bwadir/1017052001.bwa.log | samtools view -b -S -u -T $datadir/Oar_v3.1.fa -q 30 - | samtools sort -@ 1 - -o $bwadir/1017052001.bam >"$bwadir/1017052001.out" 2>"$bwadir/1017052001.out"
